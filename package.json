{
  "name": "strategy-consulting-agent",
  "version": "1.0.0",
  "description": "A strategy consulting agent powered by Claude Code SDK",
  "main": "dist/index.js",
  "bin": {
    "strategy-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js interactive",
    "start:cli": "node dist/cli.js",
    "start:api": "node dist/api/server.js",
    "dev:api": "nodemon --watch src --ext ts --exec ts-node src/api/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "strategy",
    "consulting",
    "claude",
    "ai",
    "business-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.95",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/inquirer": "^9.0.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}